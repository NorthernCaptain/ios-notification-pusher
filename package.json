    "@mui/material": "^5.6.2",
    "@mui/system": "^5.6.2",
    "@mui/x-date-pickers": "^5.0.0-alpha.1",
    "@parse/node-apn": "^5.1.3",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "postcss": "^8.4.8",
    "prismjs": "^1.28.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^12.0.5",
    "react-simple-code-editor": "^0.11.0",
    "uuid": "^8.3.2"
